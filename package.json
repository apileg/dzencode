{
    "name": "dzencode",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "up": "cd ./docker && NGINX_PORT=3001 docker-compose up",
        "down-all": "cd ./docker && docker-compose down --volumes --rmi all",
        "down": "cd ./docker && docker-compose down --volumes --rmi local",
        "dev": "next dev",
        "lint": "next lint",
        "build": "next build",
        "start": "next start",
        "db": "docker-compose -f ./docker/docker-compose.mysql.yml",
        "db:setup": "npm run db:migrate && npm run db:seed",
        "db:start": "docker-compose -f ./docker/docker-compose.mysql.yml up -d",
        "db:stop": "npm run db down",
        "db:seed": "tsx ./scripts/db/seed.ts",
        "db:migrate": "tsx ./scripts/db/migrate.ts",
        "db:dump": "mysqldump dzencode --host 127.0.0.1 --port 3307 --no-data > dzencode.sql",
        "docker:build": "cd ./docker && docker-compose build app",
        "docker:save": "docker save docker-app > ./docker/app.tar",
        "test:api": "tsx ./scripts/tests/run-api-tests.ts",
        "test:unit": "jest ./tests/jest/unit",
        "https": "tsx ./scripts/https.ts"
    },
    "dependencies": {
        "@prisma/client": "4.14.1",
        "@tanstack/react-query": "4.29.7",
        "autoprefixer": "10.4.14",
        "bcrypt": "5.1.0",
        "execa": "7.1.1",
        "jose": "4.14.4",
        "next": "13.4.2",
        "postcss": "8.4.23",
        "prisma": "4.14.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "socket.io": "4.6.1",
        "socket.io-client": "4.6.1",
        "tailwindcss": "3.3.2",
        "tsx": "3.12.7",
        "typescript": "5.0.4",
        "zod": "3.21.4",
        "zustand": "4.3.8"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "14.0.0",
        "@types/bcrypt": "5.0.0",
        "@types/jest": "29.5.1",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/set-cookie-parser": "2.4.2",
        "axios": "1.4.0",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.2",
        "jest": "29.5.0",
        "jest-environment-jsdom": "29.5.0",
        "prisma-erd": "^0.1.1",
        "set-cookie-parser": "2.6.0",
        "ts-jest": "29.1.0"
    }
}
